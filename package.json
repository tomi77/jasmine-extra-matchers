{
  "name": "jasmine-extra-matchers",
  "version": "0.1.1",
  "description": "A set of Jasmine matchers",
  "main": "dist/jasmine-extra-matchers.js",
  "scripts": {
    "test": "node --require coffee-script/register ./node_modules/.bin/jasmine --config=test/support/jasmine.json",
    "coveralls": "ibrik cover jasmine --default-excludes --files.include=\"src/*.coffee\" -- --config=test/support/jasmine.json && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "build": "npm run build-js; npm run uglify",
    "build-js": "coffee --bare --compile --no-header --output dist/ src/jasmine-extra-matchers.coffee",
    "uglify": "uglifyjs dist/jasmine-extra-matchers.js --output dist/jasmine-extra-matchers.min.js --compress --mangle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomi77/jasmine-extra-matchers.git"
  },
  "keywords": [
    "jasmine",
    "matcher"
  ],
  "author": "Tomasz Jakub Rup",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomi77/jasmine-extra-matchers/issues"
  },
  "homepage": "https://github.com/tomi77/jasmine-extra-matchers",
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "coveralls": "^2.13.1",
    "ibrik": "^2.0.0",
    "jasmine": "^2.6.0",
    "uglify-js": "^3.0.0"
  }
}
